{
  "name": "rm-discord-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --build",
    "clean": "npm run clean:db && npm run clean:dist",
    "clean:db": "rimraf data",
    "clean:dist": "rimraf dist",
    "dev": "tsx src/dev.ts",
    "start": "node .",
    "start:dev": "tsc-watch --noColors --onSuccess \"npm run start\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@discordx/importer": "^1.3.3",
    "@discordx/utilities": "^8.0.0",
    "cron": "^4.3.4",
    "discord.js": "^14.25.1",
    "discordx": "^11.13.2",
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "pg-query-stream": "^4.10.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28",
    "upgrade": "^1.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@typescript-eslint/typescript-estree": "^8.47.0",
    "@typescript-eslint/utils": "^8.47.0",
    "chokidar": "^4.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "tsc-watch": "^7.2.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=25.0.0",
    "npm": ">=11.0.0"
  }
}
